        case 77: break;
        case 51: 
          {  return symbolFactory.newSymbol("BOOLEAN", BOOLEAN, Boolean.parseBoolean(yytext())); }
        case 78: break;
        case 60: 
          {  return symbolFactory.newSymbol("DENTERO", DENTERO); }
        case 79: break;
        case 54: 
          {  return symbolFactory.newSymbol("DREAL", DREAL); }
        case 80: break;
        case 17: 
          {  return symbolFactory.newSymbol("TIMES", TIMES);  }
        case 81: break;
        case 7: 
          {  return symbolFactory.newSymbol("MINUS", MINUS);  }
        case 82: break;
        case 37: 
          {  return symbolFactory.newSymbol("DECLA", DECLA);  }
        case 83: break;
        case 2: 
        case 8: 
        case 10: 
        case 13: 
        case 14: 
        case 15: 
        case 21: 
          {  emit_warning("Unrecognized character '" +yytext()+"' -- ignored");  }
        case 84: break;
        case 44: 
          {  return symbolFactory.newSymbol("CHAR", CHAR, yytext().charAt(1) ); }
        case 85: break;
        default: 
          if (yy_input == YYEOF && yy_startRead == yy_currentPos) {
            yy_atEOF = true;
            yy_do_eof();
              {     return symbolFactory.newSymbol("EOF",sym.EOF);
 }
          } 
          else {
            yy_ScanError(YY_NO_MATCH);
          }
      }
    }
  }


}
